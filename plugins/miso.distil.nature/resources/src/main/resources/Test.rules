Artifact ATLTranf [
	extension .atl 
	format ATL 
	services : download, read, readAll, search, update, upload
] { 
}  

Artifact Bento [
	extension .bentoz
	format XML
	services : upload, download, search, read, readAll
] {
	many inputsConcept : MetaModel   
	many outputsConcept : MetaModel 
	many transformations : ATLTranf
}   
       
Artifact MetaModel [
	extension .ecore
	format XMI
	services : upload, download
] {
	uri_meta : String    	  
}   
    
Artifact Model [
	format XML
	services : read, search, upload, download, update
] { 
	uri_model : String
	type : MetaModel  
}

MultiService Analysis {
	when : Bento [upload]
	services : TypeAnalysis, Metrics
}

MultiService MultiSec {
	when : MetaModel [upload, download]
	when : Model [upload, download, update]
	services : Load, Download
}
 
Service TypeAnalysis {
	output : Model [with type.uri_meta = "http://problems.ecore"]
}

Service Metrics {
	input : Model [with type.uri_meta = "http://problems.ecore"]
	output : Model [with type.uri_meta = "http://metrics.ecore"]
}